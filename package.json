{
  "name": "ag-grid-poc-monorepo",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "npm-run-all -p dev:react dev:vue dev:overview",
    "dev:react": "npm start --prefix react-app",
    "dev:vue": "npm run dev --prefix vue-app",
    "dev:overview": "npm run dev --prefix poc-overview",
    "install:all": "npm i --prefix react-app && npm i --prefix vue-app && npm i --prefix poc-overview"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
